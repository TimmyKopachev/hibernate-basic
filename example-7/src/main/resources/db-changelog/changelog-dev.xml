<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

    <changeSet id="initial-GIS-on-db-cluster" author="dzmitry.kapachou">
        <sql>
            CREATE ALIAS IF NOT EXISTS H2GIS_SPATIAL
            FOR 'org.h2gis.functions.factory.H2GISFunctions.load';
        </sql>
        <sql>CALL H2GIS_SPATIAL();</sql>
        <rollback>
            <sql>DROP ALIAS IF EXISTS H2GIS_SPATIAL;</sql>
        </rollback>
    </changeSet>


    <include relativeToChangelogFile="true" file="changelog-ddl.xml"/>
    <include relativeToChangelogFile="true" file="changelog-data.xml"/>

</databaseChangeLog>